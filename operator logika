package fariedprayugo;

public class operatorlogika {
    public static void main(String[] args) {
        boolean a = true;
        boolean b = false;
        boolean c;

        // &&(dan)
        c = a && b;
        System.out.println("true && false =" + c);

        //  || (atau)
        c = a || b;
        System.out.println("true || false = " + c);

        // ! (bukan)
        System.out.println("false, !true = " + !a);

    }
}
